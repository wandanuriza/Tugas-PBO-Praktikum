package Association;

public class main {
	public static void main (String[] args) 
	{
		Lecturer lctr = new Lecturer("Wanda");
		Course cs1 = new Course("Matematika Terapan");
		Course cs2 = new Course("Pemrograman Berorientasi Objek");
		Course cs3 = new Course("PKWU");
		
		lctr.addCourse(cs1);  //ada relasi asosiasi melalui method addCourse
		lctr.addCourse(cs2);
		lctr.addCourse(cs3);
		lctr.printInfo();
	}	
}

